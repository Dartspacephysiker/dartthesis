@incollection{Ostgaard2015,
abstract = {Auroral studies have shown that there are systematic displacements and significant intensity differences of the aurora in the two hemispheres. Such observations have been systematically correlated with the various components of the interplanetary magnetic field (IMF) and hemispherical differences in solar exposure. This chapter reviews some new results on nonconjugate phenomena that have been reported the last couple of years. It focuses on the three suggested mechanisms to produce asymmetric aurora in the conjugate hemispheres. The mechanisms discussed in the chapter are: difference in region 1 currents due to hemispheric differences in the solar wind dynamo efficiency when the IMF has a significant Bx component; interhemispheric or asymmetric currents associated with the “penetration” of the IMF By component into the closed magnetosphere; and interhemispheric currents due to conductivity differences in the two hemispheres.},
author = {{\O}stgaard, Nikolai and Reistad, Jone Peter and Tenfjord, Paul and Laundal, Karl Magnus and Snekvik, Kristian and Milan, Steve and Haaland, Stein},
booktitle = {Auroral Dynamics and Space Weather},
