journal = {Journal of Geophysical Research: Space Physics},
keywords = {2407 Auroral ionosphere,2483 Wave/particle interactions,2736 Magnetosphere/ionosphere interactions,Diffuse aurora,Distribution functions,M-I coupling,diffuse aurora,electron distribution,pulsating aurora,wave-particle interaction},
mendeley-tags = {Diffuse aurora,Distribution functions,M-I coupling,pulsating aurora},
month = {nov},
