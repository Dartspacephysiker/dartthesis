file = {:home/spencerh/Desktop/ostgaard{\_}et{\_}al{\_}Monograph{\_}2015.pdf:pdf},
isbn = {9781118978719},
keywords = {auroral asymmetries,closed magnetosphere,conjugate hemispheres,interhemispheric currents,interplanetary magnetic field,nonconjugate phenomena,solar wind dynamo efficiency},
pages = {131--143},
publisher = {John Wiley {\&} Sons, Inc},
title = {{Mechanisms that Produce Auroral Asymmetries in Conjugate Hemispheres}},
url = {http://dx.doi.org/10.1002/9781118978719.ch10},
year = {2015}
