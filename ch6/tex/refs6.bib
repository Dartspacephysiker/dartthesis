journal = {Ann. Geophys.},
keywords = {Conductance,Freja,Kappa},
mendeley-groups = {Recalculation{\_}of{\_}J-V},
mendeley-tags = {Conductance,Freja,Kappa},
number = {3},
pages = {298--302},
title = {{Field-aligned conductance values estimated from Maxwellian and kappa distributions in quiet and disturbed events using Freja electron data}},
