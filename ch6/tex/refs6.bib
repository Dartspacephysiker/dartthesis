mendeley-groups = {IMF},
mendeley-tags = {Garden-hose IMF,IMF,Parker spiral},
number = {2},
pages = {258--328},
title = {{The interplanetary magnetic field. Solar origin and terrestrial effects}},
url = {http://dx.doi.org/10.1007/BF00227565},
volume = {8},
