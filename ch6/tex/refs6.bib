file = {:home/spencerh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gershman et al. - 2015 - The calculation of moment uncertainties from velocity distribution functions with random errors.pdf:pdf;:home/spencerh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gershman et al. - 2015 - The calculation of moment uncertainties from velocity distribution functions with random errors(2).pdf:pdf},
issn = {2169-9402},
journal = {Journal of Geophysical Research: Space Physics},
keywords = {0654 Plasmas,0694 Instruments and techniques,2164 Solar wind plasma,2467 Plasma temperature and density,5443 Magnetospheres,particle instrumentation,plasma moments,random errors,velocity distribution function},
mendeley-groups = {Mathy{\_}stuff},
