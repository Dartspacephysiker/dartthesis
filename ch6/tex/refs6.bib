journal = {Journal of Geophysical Research},
keywords = {2704 magnetospheric physics: Auroral phenomena (24,2712 magnetospheric physics: Electric fields (2411,2748 magnetospheric physics: Magnetotail boundary,2752 magnetospheric physics: MHD waves and instabi},
