doi = {10.1002/jgra.50520},
file = {:home/spencerh/Desktop/Merkin{\_}et{\_}al-2013-Journal{\_}of{\_}Geophysical{\_}Research-{\_}Space{\_}Physics.pdf:pdf},
issn = {2169-9402},
journal = {J. Geophys. Res. Sp. Phys.},
keywords = {2724 Magnetopause and boundary layers,2728 Magnetosheath,2752 MHD waves and instabilities,2753 Numerical modeling,2784 Solar wind/magnetosphere interactions,Kelvin-Helmholtz Instability,LFM,Lyon-Fedder-Mobarry,global MHD,high resolution,magnetosphere},
